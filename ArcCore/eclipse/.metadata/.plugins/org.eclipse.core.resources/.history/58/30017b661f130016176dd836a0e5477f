package com.thearclightcrusader.ArcCore.dimensions;

import com.thearclightcrusader.ArcCore.worldgen.BiomesAC;

import net.minecraft.world.WorldProvider;
import net.minecraft.world.biome.WorldChunkManager;
import net.minecraft.world.biome.WorldChunkManagerHell;

public class WorldProviderFairy extends WorldProvider{

	public WorldProviderFairy(){
		
	}
	
	public void registerWorldChunkManager(){
		this.worldChunkMgr = new WorldChunkManagerHell(BiomesAC.biomeGildedPlains, 1.2F);
		this.dimensionId = DimensionRegistry.dimensionId;
	}
	
	public String getDimensionName() {
		// TODO Auto-generated method stub
		return null;
	}

}
